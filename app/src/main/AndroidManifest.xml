<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo"
        android:label="Bupasta"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"

        android:theme="@style/Theme.UASDAA"
        tools:targetApi="31">
        <activity
            android:name=".presentation.page.intro.IntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.page.home.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.page.auth.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.page.auth.LoginActivity"
            android:exported="false"/>
        <activity
            android:name=".OptimasiBiaya"
            android:exported="false" />
    </application>

</manifest>